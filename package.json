{
  "private": true,
  "version": "0.0.1",
  "name": "davidwells.io",
  "homepage": "https://davidwells.io",
  "repository": "https://github.com/DavidWells/davidwells.io.git",
  "phenomic": {
    "CNAME": true,
    "source": "dist-content",
    "offline": false,
    "cache": false
  },
  "scripts": {
    "lintfix": "eslint --ignore-path .gitignore --fix ./src",
    "lint:js": "eslint --ignore-path .gitignore src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "prepareContent": "node scripts/content",
    "dev": "npm run prepareContent && phenomic start",
    "start": "npm-run-all --parallel dev",
    "prebuild": "npm run prepareContent",
    "build": "phenomic build && npm run shrink",
    "shrink": "node scripts/minify.js",
    "postbuild": "node scripts/redirects.js && cp -R resume dist",
    "static": "cd dist && http-server",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-minify": "^0.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "classnames": "^2.2.3",
    "color": "^0.11.1",
    "copy-webpack-plugin": "^3.0.1",
    "critical": "^0.8.0",
    "css-loader": "^0.23.0",
    "cssnano": "^3.7.7",
    "directory-tree": "^1.1.0",
    "download": "^5.0.2",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "git://github.com/ServerlessInc/extract-text-webpack-plugin.git#webpack-1",
    "file-loader": "^0.8.1",
    "fs-extra": "^0.30.0",
    "gray-matter": "^2.0.2",
    "history": "^2.0.0",
    "htmlparser2": "^3.9.1",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "markdown-magic": "^0.1.12",
    "node-dir": "^0.1.16",
    "npm-run-all": "^1.7.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.7.0",
    "postcss-math": "0.0.5",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "postcss-simple-vars": "^3.0.0",
    "prepend-file": "^1.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sitemap": "^1.8.1",
    "style-loader": "^0.12.3",
    "stylelint": "^6.8.0",
    "stylelint-config-standard": "^10.0.0",
    "watch": "^1.0.1",
    "webpack": "^1.12.1",
    "yamljs": "^0.2.8"
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "block-no-empty": null
    }
  },
  "dependencies": {
    "analytics": "0.0.9",
    "analytics-plugin-ga": "0.0.12",
    "auth0-lock": "10.2.1",
    "axios": "^0.13.1",
    "classnames": "^2.2.5",
    "clipboard": "^1.5.16",
    "jwt-decode": "^2.1.0",
    "localforage": "^1.5.0",
    "lodash.debounce": "^4.0.8",
    "phenomic": "0.20.2",
    "react": "^15.0.0-rc.1",
    "react-auto-form": "^1.3.0",
    "react-dom": "^15.0.0-rc.1",
    "react-google-analytics": "^0.2.0",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "redux": "^3.0.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^0.11.0"
  },
  "dllPlugin": {
    "path": "node_modules/@dummy-dll-folder"
  }
}
